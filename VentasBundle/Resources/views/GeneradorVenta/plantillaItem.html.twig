<div class="item">
	<div class="item_contenido primero">
		<input type="text" value="{{ existencia.referencia }}" disabled>
	</div>			
	<div class="item_contenido item_titulo">
		<input type="text" value="{{ existencia.getObjetoVinculado().titulo }}" disabled>
	</div>
{# 	<div class="item_contenido item_unidades">#}
{# 		<input type="text" onfocus="funcion_onfocus(this, 'Unid.');" onblur="funcion_onblur(this, 'Unid.');" value="Unid.">#}
{# 	</div>#}
	<div class="item_contenido item_precio">
		<input type="text" value="{{ existencia.precio * ( 1 + existencia.beneficio ) }}" disabled>€
	</div>
	<div class="item_contenido item_iva">
		<input type="text" value="{{ existencia.iva * 100 }}" disabled>%
	</div>
	<div class="item_contenido item_pvp">
		<input type="text" value="{{ existencia.pvp }}" disabled>€
	</div>
	<div class="item_contenido item_descuento">
		<input type="text" value="{% if existencia.itemVenta is not null %}{{ existencia.itemVenta.descuento }}{% else %}0{% endif %}">%
	</div>
	<div class="item_contenido item_precio_total">
		<input type="text" onfocus="funcion_onfocus(this, 'Total');" onblur="funcion_onblur(this, 'Total');" value="Total">€
	</div>
	
	<input type="submit" class="boton borrar_Articulo" value="-">
</div>